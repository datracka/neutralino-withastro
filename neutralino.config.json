{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/astro/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*"],
  "modes": {
    "window": {
      "title": "neutralino-withastro",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/astro/public/favicon.svg"
    }
  },
  "cli": {
    "binaryName": "neutralino-withastro",
    "resourcesPath": "/astro/dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/astro/public/neutralino.js",
    "binaryVersion": "4.9.0",
    "clientVersion": "3.8.0"
  }
}
